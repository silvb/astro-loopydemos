---
import type { LineLabel } from "@types"
import { LineLabelState } from "./line-label-state"
import LineLabelLayout from "./line-label-layout.astro"

interface Props extends LineLabel {
  pedalSlug: string
}

const { id, pedalSlug, dependency } = Astro.props
---

<LineLabelState {id} {pedalSlug} {dependency} client:only="solid-js">
  <LineLabelLayout {...Astro.props} />
</LineLabelState>
