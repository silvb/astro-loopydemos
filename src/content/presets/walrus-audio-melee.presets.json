{
  "presets": [
    {
      "id": "addictive_shoegaze",
      "label": "Addictive Shoegaze",
      "settings": {
        "vol": 7,
        "tone": 3,
        "decay": 1,
        "order": 3,
        "reverb_mode": 3,
        "mix_dist": { "angle": 0, "radius": 0.7 },
        "reverb_mode_led": true
      }
    },
    {
      "id": "cinematic_vibes",
      "label": "Cinematic Vibes",
      "settings": {
        "vol": 7,
