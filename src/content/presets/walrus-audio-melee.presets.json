      }
    },
    {
      "id": "tone_toggle",
      "label": "Tone",
      "isSweep": true,
      "target": "tone",
      "initialValue": 1,
      "values": [1, 2, 3],
      "settings": {
        "vol": 7,
        "decay": 1,
        "order": 1,
        "reverb_mode": 3,
        "mix_dist": { "angle": 0, "radius": 0 },
