    {
      "id": "decay_toggle",
      "label": "Decay",
      "isSweep": true,
      "target": "decay",
      "initialValue": 1,
      "values": [1, 2, 3],
      "settings": {
        "vol": 8,
        "tone": 1,
        "order": 1,
        "reverb_mode": 2,
        "mix_dist": { "angle": 80, "radius": 1.1 },
        "reverb_mode_led": true
      }
    },
    {
      "id": "mod_toggle",
      "label": "Modulation",
      "isSweep": true,
      "target": "decay",
      "initialValue": 1,
      "values": [1, 2, 3],
      "settings": {
        "vol": 8,
        "tone": 1,
        "order": 3,
        "reverb_mode": 1,
        "decay_label": "hold down bypass switch and this controls the amount of modulation",
        "mix_dist": { "angle": 60, "radius": 1.4 },
        "reverb_mode_led": true
      }
    },
    {
      "id": "order_toggle",
